@{
    ViewBag.Title = Resource.Title_CreateAccountComplete + " | LikeOrder.com";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var culture = System.Threading.Thread.CurrentThread.CurrentUICulture.Name.ToLowerInvariant();
}

@section Header{
    @Html.Partial("~/Views/Partial/_Header.cshtml")
}

<section class="forget-p">
    <h1 class="text-center">@Resource.Home_RegisterSuccess</h1>
    <div class="forget-p-yanzheng">
        <img src="~/Images/success-icon-10.png" atl="" width="100" style="margin-top: 10px;" alt="" />
        <p class="text-center" style="padding: 15px 0; font-size: 16px;">
          @Resource.Register_CompleteRegister
        <p class="text-center" style="padding: 15px 0; font-size: 14px;">@Resource.Register_NoteHomeRegister</p>
    </div>
    <div class="text-center back-to-home">
        <a href="/@culture">@Resource.Home_Home </a>|<a href="@Url.Action("Login","Account")">@Resource.Home_Login</a>
    </div>
</section>