@section scripts{
    @*<script src="~/Content/js/loadToolbarPartial.js"></script>*@

    <script>
        $(document).ready(function () {
            var priceParam = parse('price');
            if (priceParam == 'free') {
                $('#dvPrice a').removeClass("active");
                $('#dvPrice a.free').addClass("active");
            } else if (priceParam == 'paid') {
                $('#dvPrice a').removeClass("active");
                $('#dvPrice a.paid').addClass("active");
            } else {
                $('#dvPrice a').removeClass("active");
                $('#dvPrice a.all').addClass("active");
            }

            setTimeout(function () {
                var collectionParam = parse('collection');
                if (collectionParam != null && collectionParam != '') {
                    $('#CollectionId').val(collectionParam);
                    $('#CollectionId option[value="' + collectionParam + '"]').attr('selected', 'selected');
                    $(document).find('#dvColletion li[data-original-index="' + collectionParam + '"]').addClass('background-selected');
                    $(document).find('#dvColletion li[data-original-index="0"]').removeClass('selected');
                    $(document).find('#dvColletion button[data-id="CollectionId"]').attr('title', '');
                    if ($("#CollectionId option:selected").text().length > 20) {
                        var sel_text = $("#CollectionId option:selected").text().substr(0, 15);
                        $("#CollectionId").parent().find(".bootstrap-select .filter-option").text(sel_text + "...");
                    } else {
                        $("#CollectionId").parent().find(".bootstrap-select .filter-option").text($("#CollectionId option:selected").text());
                    }
                    //We need to show the text inside the span that the plugin show

                }

                //var collectionParam = parse('collection');
                //if (collectionParam.length) {
                //    if(collectionParam!='0')
                //    $('#dvColletion ul li:first').hide();
                //}

                var origin_text = $("#CollectionId").parent().find(".bootstrap-select .filter-option").text();
                if ($(window).width() >= 992 && $(window).width() < 1200) {
                    if (origin_text.length > 10) {
                        var temp_text = origin_text.substr(0, 9);
                        $("#CollectionId").parent().find(".bootstrap-select .filter-option").text(temp_text + "...");
                    }
                } else if ($(window).width() >= 1200) {
                    if (origin_text.length > 20) {
                        var temp_text = origin_text.substr(0, 17);
                        $("#CollectionId").parent().find(".bootstrap-select .filter-option").text(temp_text + "...");
                    }
                }


                var sortParam = parse('sort');
                if (sortParam != null && sortParam != '') {
                    $('#SortId').val(sortParam);
                    $('#SortId option[value="' + sortParam + '"]').attr('selected', 'selected');
                    $(document).find('#dvSort li[data-original-index="' + sortParam + '"]').addClass('background-selected');
                    $(document).find('#dvSort li[data-original-index="0"]').removeClass('selected');
                    $(document).find('#dvSort button[data-id="SortId"]').attr('title', '');
                    //We need to show the text inside the span that the plugin show
                    var sortText = $("#SortId option:selected").text();
                    if ($(window).width() >= 992 && $(window).width() < 1200) {
                        if (sortText.length > 10) {
                            sortText = sortText.substr(0, 9);
                        }
                    }
                    $("#SortId").parent().find(".bootstrap-select .filter-option").text(sortText);

                }

            }, 300);

            var searchParam = parse('search');
            $("#search").val(searchParam);

            $("#btnSearch").click(function () {
                $("#formSearch").submit();
            });
            //var dropdownList = $("#SearchId");

            $("#SortId").change(function () {
                loadTheme();
            });

            $("#CollectionId").change(function () {
                loadTheme();
            });

            $('#dvPrice a').click(function () {
                $('#dvPrice a').removeClass("active");
                $(this).addClass("active");
                //console.log('ok');
                loadTheme();

                return false;
            });


        });

        function parse(val) {
            var result = "",
                tmp = [];
            location.search
                //.replace ( "?", "" )
                // this is better, there might be a question mark inside
                .substr(1)
                .split("&")
                .forEach(function (item) {
                    tmp = item.split("=");
                    if (tmp[0] === val) result = decodeURIComponent(tmp[1]);
                });
            return result;
        }
    </script>

