            $(document).bind('DOMMouseScroll', function (e) {
                if (e.originalEvent.detail <= 0) {
                    if ($(this).scrollTop() >= $('.hot-theme').offset().top) {
                        $(".scroll-top-bar").show();
                    }
                    else {
                        $(".scroll-top-bar").hide();
                    }
                    if ($(this).scrollTop() >= $('.guide-step').offset().top) {
                        $(".scroll-top-bar").hide();
                    }
                }
                else {
                    $(".scroll-top-bar").hide();
                }
            });
            //IE, Opera, Safari
            $(document).bind('mousewheel', function (e) {
                if (e.originalEvent.wheelDelta >= 0) {
                    if ($(this).scrollTop() >= $('.hot-theme').offset().top) {
                        $(".scroll-top-bar").show();
                    }
                    else {
                        $(".scroll-top-bar").hide();
                    }
                    if ($(this).scrollTop() >= $('.guide-step').offset().top) {
                        $(".scroll-top-bar").hide();
                    }
                }
                else {
