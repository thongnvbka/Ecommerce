<div id="PartnerAddOrEdit" class="modal fade modal-dialog modal-p60" role="dialog">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Detail shipping partner</h4>
    </div>
    <div class="modal-body">
        <!-- Table row -->
        <div class="row importwarehouse">
            <div class="row">
                <div class="col-md-12">
                    <!-- Horizontal Form -->
                    <div class="box box-danger">
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal" id="inforNoti">
                            <div class="col-md-12">
                                <div class="box-body">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Name of the carrier: <span class="color-red">(*)</span></label>
                                        <div class="col-sm-9">
                                            <input data-bind="value: partnerModel().Name" type="text" id="Name" name="Name" class="form-control">
                                            <input data-bind="value: partnerModel().Id" type="hidden">
                                        </div>
                                    </div>
                                    <div class="form-group"> 
                                        <label class="col-sm-3 control-label">Carrier code: <span class="color-red">(*)</span></label>
                                        <div class="col-sm-9">
                                            <input data-bind="value: partnerModel().Code" type="text" id="Code" name="Code"  class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Chronological order:</label>
                                        <div class="col-sm-9">
                                            <input data-bind="value: partnerModel().PriorityNo" type="number" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Status :<span class="color-red">(*)</span></label>
                                        <div class="col-sm-9">
                                            <select class="form-control select-view ddlAdd-system" id="ddlPage" style="width: 100%;"
                                                    data-bind="options: listStatus, optionsText: 'Text',optionsValue: 'Value', value: partnerModel().Status"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Notes: </label>
                                        <div class="col-sm-9">
                                            <input data-bind="value: partnerModel().Description" type="text" class="form-control">
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </form>
                    </div>
                    <!-- /.box -->
                </div>
            </div>
            <!-- /.col -->
        </div>
    </div>
    <div class="modal-footer">
        <a class="btn btn-success" id="btnEdit2" data-bind="click: savePartner"><i class="fa fa-plus"></i>Save</a>
        <a class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</a>
    </div>
</div>


