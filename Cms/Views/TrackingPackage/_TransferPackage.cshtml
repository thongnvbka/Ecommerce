sss<div id="transferPackageModal" class="modal fade modal-dialog modal-p50" tabindex="-1" data-backdrop="static" role="dialog">
    <form data-bind="submit: save" id="transferPackageForm" class="form-horizontal">
        @Html.AntiForgeryToken()
        <div class="modal-body">
            <div class="row">
                <div class="col-xs-9">
                    <h2 class="order-header">
                        <i class="fa fa-plus"></i>
                        <span>Tranfer of package to other orders</span>
                    </h2>
                </div>
            </div>
            <!-- info row -->
            <div class="row">
                <div class="col-xs-12">
                    <div class="form-group">
                        <label for="code" class="col-sm-2 control-label">Packages code:</label>
                        <div class="col-sm-10">
                            <p data-bind="text: 'P'+code()" class="form-control-static"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="transportCode" class="col-sm-2 control-label">Transport code:</label>
                        <div class="col-sm-10">
                            <p data-bind="text: transportCode" class="form-control-static"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="customerEmail" class="col-sm-2 control-label">Email :</label>
                        <div class="col-sm-10">
                            <p data-bind="text: customerEmail" class="form-control-static"></p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="chooseOrder" class="col-sm-2 control-label">Tranfer to orders <span class="color-red">*</span>:</label>
                        <div class="col-sm-10">
                            <suggetion-order-input-components params="value: { callback: chooseOrder}"></suggetion-order-input-components>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="note" class="col-sm-2 control-label">Note<span class="color-red">*</span>:</label>
                        <div class="col-sm-10">
                            <textarea data-bind="value: note" id="note" rows="5" class="form-control"></textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button data-bind="disable: isLoading" class="btn btn-success" type="submit"><i data-bind="attr: {class: isLoading() ? 'fa fa-spinner fa-pulse' : 'fa fa-save'}" class="fa fa-save"></i>
                <span></span>Add package </button>
            <a class="btn btn-default" data-dismiss="modal">Close</a>
        </div>
    </form>
</div>