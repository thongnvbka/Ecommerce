@model CategoryMeta

@{
    ViewBag.Title = "Add new warehouse";
}

<script>
    var userTree = @Html.Raw(ViewBag.UserTree);
</script>

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Add new warehouse
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Warehouse")">List of warehouses</a></li>
        <li class="active">Add new warehouse</li>
    </ol>
</section>
<section class="content">
    <div class="box box-info">
        <form action="@Url.Action("WarehouseCreate")" method="post" class="form-horizontal" id="submitForm">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_WarehouseUpdate", Model)
            </div>
            <div class="box-footer">
                <a href="@Url.Action("Warehouse")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i> Back to list</a>
                <button type="submit" class="btn btn-info pull-right"><i class="fa fa-plus"></i> Add warehouse</button>
            </div>
        </form>
    </div>
</section>