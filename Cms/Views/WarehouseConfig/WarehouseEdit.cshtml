@model WarehouseMeta

@{
    ViewBag.Title = string.Format("Update the warehouse \"{0}\"", Model.Code);
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        @ViewBag.Title
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="@Url.Action("Warehouse")">List of warehouses</a></li>
        <li class="active">Update the warehouse</li>
    </ol>
</section>

<section class="content">
    <div class="box box-info">
        <!-- /.box-header -->
        <!-- form start -->
        <form action="@Url.Action("WarehouseEdit")" method="post" class="form-horizontal">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_WarehouseUpdate", Model)
            </div>
            <!-- /.box-body -->
            <div class="box-footer">
                <a href="@Url.Action("Warehouse")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i> Back to list</a>
                <button type="submit" class="btn btn-info pull-right"><i class="fa fa-save"></i> Save</button>
            </div>
            <!-- /.box-footer -->
        </form>
    </div>
</section>
