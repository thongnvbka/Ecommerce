<script type="text/html" id="packing-list">
    <tr>
        <td data-bind="text: (page-1)*pagesize + $index()+1" class="text-center"></td>
        <td class="text-center">
            <a data-bind="click: $parent.viewPackingDetailModal" href="javascript:;"><strong data-bind="text: Code"></strong></a>
        </td>
        <td class="text-center">
            <i class="fa fa-clock-o"></i><span data-bind="text: moment(Created).locale('vi').startOf().fromNow()" class=""></span>
        </td>
        <td class="text-center" data-bind="text: PackingListName"></td>
        <td class="text-center">
            <strong data-bind="text: WalletNumber"></strong>
        </td>
        @*<td class="text-center" data-bind="text: moment(TimeStart).locale('en').format('MM/DD/YYYY hh:mm A')">--/--/---- --:--:--</td>
        <td class="text-center" data-bind="text: moment(TimeEnd).locale('en').format('MM/DD/YYYY hh:mm A')">--/--/---- --:--:--</td>*@
        <td>
            <span data-bind="text: 'From ' + moment(TimeStart).locale('en').format('MM/DD/YYYY hh:mm A')" ></span><br />
            <span data-bind="text: 'To: ' + moment(TimeEnd).locale('en').format('MM/DD/YYYY hh:mm A')"></span>
        </td>
        <td>
            <span>From: <strong data-bind="text: WarehouseSourceName"></strong></span><br />
            <span>To: <strong data-bind="text: WarehouseDesName"></strong></span>
        </td>
        <td class="text-center">
            <span data-bind="text: statusApp.packinglist[Status].Name, css: statusApp.packinglist[Status].Class"></span>
        </td>
        <td class="text-center">
            <a href="#" class="btn btn-sm btn-primary btn-xs"><i class="fa fa-edit"></i>Edit</a>
            <a href="#" class="btn btn-sm btn-danger btn-xs"><i class="fa fa-trash"></i> Delete </a>
        </td>
    </tr>

</script>