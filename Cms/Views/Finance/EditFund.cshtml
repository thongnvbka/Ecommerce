@model Library.Models.FinanceFundData
@{
    ViewBag.Title = string.Format("Update fund information \"{0}\"", Model.Name);
}
<script>
    var financeFundJsTree = @Html.Raw(ViewBag.financeFundJsTree);
</script>
<section class="content-header">
    <h1>
        Update fund information
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Fund")">Fund list</a></li>
        <li class="active">Update fund</li>
    </ol>
</section>
<section class="content">
    <div class="box box-info">
        <form action="@Url.Action("EditFund")" method="post" class="form-horizontal" id="submitForm">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_Update", Model)
            </div>
            <div class="box-footer">
                <a href="@Url.Action("Fund")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i> Back to list</a>
                <button type="button" class="btn btn-info pull-right" onclick="submitForm()"><i class="fa fa-plus"></i> Update fund</button>
            </div>
        </form>
    </div>
</section>

