@model TreasureMeta
@{
    ViewBag.Title = "Add new revenue statement";
}
<script>
    var treasureJsTree = @Html.Raw(ViewBag.treasureJsTree);
</script>
<section class="content-header">
    <h1>
        Add new revenue statement
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("AccountingRegulations")">Fund list</a></li>
        <li class="active">Add new revenue statement</li>
    </ol>
</section>
<section class="content">
    <div class="box box-info">
        <form action="@Url.Action("CreateTreasure")" method="post" class="form-horizontal" id="submitForm">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_UpdateTreasure", Model)
            </div>
            <div class="box-footer">
                <a href="@Url.Action("AccountingRegulations")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i> Back to list</a>
                <button type="button" class="btn btn-info pull-right" onclick="submitForm()"><i class="fa fa-plus"></i> Add a term</button>
            </div>
        </form>
    </div>
</section>


<!-- /.content -->

