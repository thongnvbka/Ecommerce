@{
    ViewBag.Title = "Add new transfer notes";
}
<section class="content-header"></section>
<!-- Main content -->
<section class="invoice importwarehouse">
    <!-- title row -->
    <div class="row">
        <div class="col-xs-12 text-center">
            <h2 class="page-header">
                <strong>Create transcripts</strong>
                
                
                <span class="impornumber">Slip number: <strong>FLT6896253</strong></span>
             
                <small>Date of inventory: 02/10/2016</small>
            </h2>
        </div>
        <!-- /.col -->
    </div>
    <!-- info row -->
    <div class="row importwarehouse">
        <!-- accepted payments column -->
        <div class="col-xs-6">
            <div class="table-responsive">
                <table class="table">
                    <tr>
                        <th width="25%">RECEIVER</th>
                        <td>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-user"></i>
                                </div>
                                <input type="text" class="form-control" placeholder="Receiver">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th>Phone</th>
                        <td>
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-phone"></i>
                                </div>
                                <input type="text" class="form-control" placeholder="Phone">
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <th>Address</th>
                        <td><textarea class="form-control" rows="2" placeholder="Address ..."></textarea></td>
                    </tr>
                </table>
            </div>
        </div>
        <div class="col-xs-6">
            <div class="table-responsive">
                <table class="table">
                    <tr>
                        <th width="25%">WarehouseReceipt :</th>
                        <td>Noi Bai Warehouse</td>
                    </tr>
                    <tr>
                        <th>Address :</th>
                        <td>123 Noi Bai - Ha Noi - Viet Nam</td>
                    </tr>
                </table>
            </div>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
    <div class="row importwarehouse">
        <div class="col-sm-3">
            <div class="form-group">
                <label>Bag code</label>
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </div>
                    <select class="form-control input-lg select-view" style="width: 100%;" placeholder="ssss">
                        <option selected="selected">- ENTER CODE COVER -</option>
                        <option>BH02012012</option>
                        <option>BH02011231</option>
                        <option>BH01241120</option>
                        <option>BH02230133</option>
                        <option>BH02264422</option>
                        <option>BH02024122</option>
                    </select>
                </div>
                <!-- /.input group -->
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>TOTAL BALANCE (KG)</label>
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </div>
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="For example: 2">
                </div>
                <!-- /.input group -->
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>BALANCED CONDITION (KG)</label>
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </div>
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="For example: 2">
                </div>
                <!-- /.input group -->
            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                <label>Volume (m3)</label>
                <div class="input-group">
                    <div class="input-group-addon">
                        <i class="fa fa-calendar"></i>
                    </div>
                    <input type="text" class="form-control" id="exampleInputEmail1" placeholder="For example: 2m3">
                </div>
                <!-- /.input group -->
            </div>
        </div>

        <div class="col-sm-12">
            <div class="form-group">
                <div class="alert alert-danger alert-dismissible">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="false">&times;</button>
                    <h4><i class="icon fa fa-warning"></i> Warning!</h4>
                    Daily bags are not shipped to Hanoi.
                </div>
            </div>
        </div>

        <div class="col-sm-12">
            <div class="form-group">
                <label>Note bag</label>
                <textarea class="form-control" rows="2" placeholder="Containers are fragile, recommended carefully..."></textarea>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="form-group">
                <button type="submit" class="col-sm-1 btn btn-primary">IMPORT</button>
            </div>
        </div>

    </div>
    <!-- Table row -->
