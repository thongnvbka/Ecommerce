        <div class="col-xs-12">
            <a href="@Url.Action("")" target="_blank" class="btn btn-default"><i class="fa fa-arrow-left"></i>List of transfer tickets</a>
            <a class="btn btn-danger pull-right" style="margin-right: 5px;">
