@model  Common.Items.ModelView<Library.DbContext.Entities.CustomerLevel, string>

@{
    ViewBag.Title = "Manage Level vip";
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Manage Level vip
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Index","CustomerLevel")">Manage Level vip</a></li>
        <li class="active">List level vip</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">List level vip</h3>
                    <a href="@Url.Action("Add","CustomerLevel")" class="btn btn-success pull-right"><i class="fa fa-plus"></i>Add new</a>
                </div>
                @Html.Partial("_List", Model)
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- Modal -->

@using (Html.BeginScriptBlock())
{
    <script type="text/javascript">
        $(document).ready(function () {
            @if (TempData["Msg"] != null) {
                <text>toastr.success('@Html.Raw(TempData["Msg"])');</text>
            }
        })
    </script>
}