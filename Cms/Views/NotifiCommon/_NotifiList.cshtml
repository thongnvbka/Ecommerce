<script type="text/html" id="ListNoti">
    <tr>
        <td data-bind="text: (page-1)* pagesize + $index()+1" class="text-center"></td>
        <td class="text-center">
            <a data-bind="click: $parent.viewNotifiModal" href="javascript:;"><strong data-bind="text: Title"></strong></a>
        </td>
        <td class="text-center" data-bind="text: moment(CreateDate).locale('vi').format('L')"></td>
        <td class="text-center">
            <input type="checkbox" data-bind="checked: Status" />
        </td>
        <td class="text-center" data-bind="text: SystemName"></td>
        <td class="text-center">
            <a data-bind="click: $parent.viewNotifiModal" href="javascript:;"
               class="btn btn-sm btn-primary btn-xs"><i class="fa fa-edit"></i>Edit</a>
            <a data-bind="click: $parent.removeNotifi" href="javascript:;"
               class="btn btn-danger btn-xs"><i class="fa fa-trash"></i> Delete</a>
        </td>
    </tr>
</script>