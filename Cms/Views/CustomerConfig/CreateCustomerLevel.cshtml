 @model CustomerLevelMeta
@{
    ViewBag.Title = "CreateCustomerLevel";

}
 
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Add new customer level
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Level")">Customer level list</a></li>
        <li class="active">Add new customer level</li>
    </ol>
</section>

<section class="content">
    <div class="box box-info">
        <form action="@Url.Action("CreateCustomerLevel")" method="post" class="form-horizontal" id="submitForm">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_UpdateCustomerLevel", Model)
            </div>
            <div class="box-footer">
                <a href="@Url.Action("Level")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i>Back to list</a>
                <button type="button" class="btn btn-info pull-right" onclick="submitForm()"><i class="fa fa-plus"></i>Add Category</button>
            </div>
        </form>
    </div>
</section>
