@model ComplainTypeMeta
@{
    ViewBag.Title = string.Format("Update types of complaint information \"{0}\"", Model.Name);
}
<script>
     var complainTypeJsTree = @Html.Raw(ViewBag.complainTypeJsTree);
</script>
<section class="content-header">
    <h1>
        Update types of complaint information
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("ComplainType")">List of complaint types</a></li>
        <li class="active">Update complaint types</li>
    </ol>
</section>
<section class="content">
    <div class="box box-info">
        <form action="@Url.Action("EditComplainType")" method="post" class="form-horizontal" id="submitForm">
            @Html.AntiForgeryToken()
            <div class="box-body">
                @Html.Partial("_UpdateComplainType", Model)
            </div>
            <div class="box-footer">
                <a href="@Url.Action("ComplainType")" role="button" class="btn btn-default"><i class="fa fa-arrow-left"></i> Back to list</a>
                <button type="button" class="btn btn-info pull-right" onclick="submitForm()"><i class="fa fa-plus"></i> Update complaint types</button>
            </div>
        </form>
    </div>
</section>
<!-- /.content -->
